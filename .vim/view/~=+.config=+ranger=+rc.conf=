let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
argglobal
let s:cpo_save=&cpo
set cpo&vim
xmap <buffer> P <Plug>VisualPasta
nmap <buffer> P <Plug>BeforePasta
xmap <buffer> p <Plug>VisualPasta
nmap <buffer> p <Plug>AfterPasta
let &cpo=s:cpo_save
unlet s:cpo_save
setlocal keymap=
setlocal noarabic
setlocal autoindent
setlocal balloonexpr=
setlocal nobinary
setlocal bufhidden=
