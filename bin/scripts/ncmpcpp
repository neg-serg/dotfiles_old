[ -z $(wmctrl -l|awk '{ print $4 }'|grep ncmpcpp) ] && /bin/urxvt -fn xft:Pragmata\ Pro\ for\ Powerline:pixelsize=18 -name mpd-pad2 -e ncmpcpp &
